<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/freshchat_calendar_no_timeslots_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="gone"
    tools:visibility="visible">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/freschat_calendar_no_timeslots_found_view"
        style="?attr/freshchatCalendarNoTimeSlotsFoundTextStyle" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/freshchat_calendar_cancel_invite_button"
        style="?attr/freshchatCalendarOutlineButtonStyle"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:text="@string/freshchat_calendar_cancel_invite" />
</RelativeLayout>